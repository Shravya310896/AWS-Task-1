{
  "AWSTemplateFormatVersion": "2010-09-09",
  "Description": "Process Lambda function Stack",
  "Metadata": {
    "AWS::CloudFormation::Interface": {
      "ParameterGroups": [
        {
          "Label": {
            "default": "Project Info"
          },
          "Parameters": []
        }
      ]
    }
  },
  "Parameters": {
    "lambdaFunctionHandler": {
      "Description": "The Lambda Function Handler",
      "Type": "String"
    },
    "s3CodeDeployBucket": {
      "Description": "The name of the S3 bucket containing the Lambda sample code",
      "Type": "String"
    },
    "mains3bucketandSqsStackname": {
      "Description": "The  mains3bucketandSqs Stackname",
      "Type": "String"
    }
  },
  "Mappings": {},
  "Resources": {
    "LambdaRole": {
      "Type": "AWS::IAM::Role",
      "DependsOn": [],
      "Properties": {
        "RoleName": "processlambdafunctionrole",
        "AssumeRolePolicyDocument": {
          "Version": "2012-10-17",
          "Statement": [
            {
              "Effect": "Allow",
              "Principal": {
                "Service": [
                  "lambda.amazonaws.com"
                ]
              },
              "Action": "sts:AssumeRole"
            }
          ]
        },
        "Policies": []
      }
    },
    "LambdaRolePolicy": {
      "Type": "AWS::IAM::Policy",
      "DependsOn": "LambdaRole",
      "Properties": {
        "PolicyName": "LambdaVPCAccess",
        "PolicyDocument": {
          "Version": "2012-10-17",
          "Statement": [
            {
              "Effect": "Allow",
              "Action": [
                "logs:CreateLogGroup",
                "logs:CreateLogStream",
                "logs:PutLogEvents"
              ],
              "Resource": "arn:aws:logs:*:*:*"
            },
            {
              "Effect": "Allow",
              "Action": [
                "ec2:CreateNetworkInterface",
                "ec2:DescribeNetworkInterfaces",
                "ec2:DeleteNetworkInterface"
              ],
              "Resource": [
                "*"
              ]
            }
          ]
        },
        "Roles": [
          {
            "Ref": "LambdaRole"
          }
        ]
      }
    },
    "SQSLambdaPolicy": {
      "Type": "AWS::IAM::Policy",
      "DependsOn": "LambdaRole",
      "Properties": {
        "PolicyName": "sqslambdaAccess",
        "PolicyDocument": {
          "Version": "2012-10-17",
          "Statement": [
            {
              "Sid": "Stmt1714568759291",
              "Action": [
                "sqs:DeleteMessage",
                "sqs:ReceiveMessage",
                "sqs:SendMessage"
              ],
              "Effect": "Allow",
              "Resource": "arn:aws:sqs:us-east-1:905418360713:SQSQueuessk"
            },
            {
              "Sid": "Stmt1714568809022",
              "Action": "sqs:*",
              "Effect": "Allow",
              "Resource": "arn:aws:sqs:us-east-1:905418360713:SQSQueuessk"
            }
          ]
        },
        "Roles": [
          {
            "Ref": "LambdaRole"
          }
        ]
      }
    },
    "LambdaFunction1": {
      "Type": "AWS::Lambda::Function",
      "DependsOn": [
        "LambdaRolePolicy"
      ],
      "Properties": {
        "FunctionName": "processlambdafunction",
        "Handler": {
          "Ref": "lambdaFunctionHandler"
        },
        "Role": {
          "Fn::GetAtt": [
            "LambdaRole",
            "Arn"
          ]
        },
        "Code": {
          "S3Bucket": "s3codedeploybucket-lambdacodebucket",
          "S3Key": "processlambdapythoncode.zip"
        },
        "Runtime": "python3.12",
        "MemorySize": "1024",
        "Timeout": "30",
        "VpcConfig": {
          "SecurityGroupIds": [
            "sg-05a2182e14de89ea1"
          ],
          "SubnetIds": [
            "subnet-05390bb6cc1f61873",
            "subnet-0af95431d8949ca0d"
          ]
        },
        "Environment": {
          "Variables": {
            "regionkey": {
              "Ref": "AWS::Region"
            }
          }
        },
        "TracingConfig": {
          "Mode": "Active"
        }
      }
    },
    "MyEventSourceMapping": {
      "Type": "AWS::Lambda::EventSourceMapping",
      "Properties": {
        "BatchSize": 1,
        "EventSourceArn": {
          "Fn::ImportValue": {
            "Fn::Sub": "${mains3bucketandSqsStackname}-sqsarn"
          }
        },
        "FunctionName": {
          "Ref": "LambdaFunction1"
        },
        "Enabled": true
      }
    },
    "SNSTopic": {
      "Type": "AWS::SNS::Topic",
      "Properties": {
        "DisplayName": "SNSNewTopic"
      }
    },
    "LambdaSNSPolicy": {
      "Type": "AWS::IAM::Policy",
      "DependsOn": "LambdaRole",
      "Properties": {
        "PolicyName": "sqslambdaAccess",
        "PolicyDocument": {
          "Version": "2012-10-17",
          "Statement": [
            {
              "Sid": "Stmt1714568759292",
              "Action": [
                "sns:Publish"
              ],
              "Effect": "Allow",
              "Resource": {
                "Ref": "SNSTopic"
              }
            }
          ]
        },
        "Roles": [
          {
            "Ref": "LambdaRole"
          }
        ]
      }
    }
  },
  "Outputs": {
    "lambdaArn": {
      "Value": {
        "Fn::GetAtt": [
          "LambdaFunction1",
          "Arn"
        ]
      },
      "Export": {
        "Name": {
          "Fn::Sub": "${AWS::StackName}-lambdaArn"
        }
      }
    },
    "lambdaFunction1Name": {
      "Value": {
        "Ref": "LambdaFunction1"
      },
      "Export": {
        "Name": {
          "Fn::Sub": "${AWS::StackName}-LambdaName"
        }
      }
    }
  }
}